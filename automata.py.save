def evaluateAutomaton(chain,Automaton):
	counter=0#counter that walks the chain given
	cursor=0 #counter that walks the Automaton
	while counter!=len(chain):
		subTree=Automaton[cursor]
		sizeT=len(subTree)
		curSub=0
		while sizeT!=0:
			if sizeT==1
				if chain[counter] in subTree[0]
					cursor=subTree[1]
				if chain[counter] in subTree[curSub][0]
			
		counter+=1
		
